<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.coordinatelayout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".LargeToolbarFixActivity"
            android:label="@string/sample_large_toolbar_fix"
            android:theme="@style/AppTheme.NoActionBar"
            />

        <activity
            android:name=".ScrollToolbarActivity"
            android:label="@string/sample_scroll_toolbar"
            android:theme="@style/AppTheme.NoActionBar"
            />

        <activity
            android:name=".CollapseLargeToolbarActivity"
            android:label="@string/sample_collapse_toolbar"
            android:theme="@style/AppTheme.NoActionBar"
            />
            

        <activity
            android:name=".ParallaxImageToolbarActivity"
            android:label="@string/sample_parallax_image_tollbar"
            android:theme="@style/AppTheme.NoActionBar"
            />
            

        <activity
            android:name=".ScrollCollapseLargeToolbarActivity"
            android:theme="@style/AppTheme.NoActionBar"
            />
            

        <activity
            android:name=".CustomBehaviorActivity"
            android:theme="@style/AppTheme.NoActionBar"
            />
            

        <activity
            android:name=".SnackbarActivity"
            android:label="@string/sample_snackbar"
            android:theme="@style/AppTheme.NoActionBar"
            />

        <activity
            android:name=".PagerTabsActivity"
            android:theme="@style/AppTheme.NoActionBar"
            />
            

        <activity
            android:name=".PagerTabsParallaxImageActivity"
            android:theme="@style/AppTheme.NoActionBar"
            />
            

    </application>



</manifest>